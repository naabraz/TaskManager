{
  "root": true,
  "extends": [
    "@react-native",
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "plugins": ["import"],
  "rules": {
    "import/order": [
      1,
      {
        "groups": [
          "external",
          "builtin",
          "internal",
          "sibling",
          "parent",
          "index"
        ]
      }
    ]
  },
  "settings": {
    "import/ignore": [
		  "node_modules/react-native/index\\.js$"
	  ]
  }
}
